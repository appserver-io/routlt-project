{
	"name" : "${composer.name}",
	"description" : "Rout.Lt project stub",
	"version" : "0.1.0",
	"require" : {
		"appserver-io/routlt" : "${routlt.version}",
        "jms/serializer" : "0.16.0"
	},
	"require-dev" : {
        "appserver-io/phar":"~1.0",
        "appserver-io/build":"~1.0",
        "appserver-io/messaging":"~1.0",
        "appserver-io/lang":"~1.0",
        "appserver-io-psr/mop":"~1.0",
        "appserver-io-psr/epb":"~1.0",
        "appserver-io-psr/servlet":"~1.0"
	},
	"autoload" : {
		"psr-0" : {
            "${composer.namespace}" : [
				"common/classes",
				"common/tests/",
				"WEB-INF/classes",
				"WEB-INF/tests/",
				"META-INF/classes",
				"META-INF/tests/"
			]
		}
	}
