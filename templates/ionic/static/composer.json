		"ruflin/elastica" : "~2.0",
		"jms/serializer" : "0.16.0",
		"guzzlehttp/guzzle" : "~5.0",
		"respect/validation" : "~0.8",
		"appserver-io/routlt" : "${routlt.version}",
		"doctrine/search" : "dev-master as 0.1.0",
